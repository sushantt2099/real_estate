<!DOCTYPE html>
<html>
<head>
  <title>RealEstate | <%= yield :title %></title>

  <%= stylesheet_link_tag    "application-all", media: "all", "data-turbolinks-track" => true %>

  <% if content_for? :dashboard %>

  	<%= stylesheet_link_tag "application-dashboard", media: "dashboard", "data-turbolinks-track" => true %>

  <% elsif content_for? :property_view %>

  	<%= stylesheet_link_tag "application-property-view" %>

  <% end %>

  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

<%= debug params  %>

</body>
</html>
