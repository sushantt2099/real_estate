<!DOCTYPE html>
<html>
<head>
  <title>RealEstate | <%= yield :title %></title>

  <%= stylesheet_link_tag    "application-all", media: "all", "data-turbolinks-track" => true %>

  <% if content_for? :dashboard %>

  	<%= stylesheet_link_tag "application-dashboard", media: "all", "data-turbolinks-track" => true %>

  <% elsif content_for? :property_view %>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

  	<%= stylesheet_link_tag "application-property-view", media: "all", "data-turbolinks-track" => true %>

  <% elsif content_for? :static_page %>

    <%= stylesheet_link_tag "static-pages", media: "all", "data-turbolinks-track" => true %>
  
  <% elsif content_for? :search_pages %>

    <%= stylesheet_link_tag "search_pages", media: "all", "data-turbolinks-track" => true %>

  <% end %>

  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="content">
		<%= yield %>
	</div>

</body>
</html>
